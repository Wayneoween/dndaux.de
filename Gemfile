source 'https://rubygems.org'

# Use GitHub Pages gem which includes Jekyll and compatible versions
gem 'github-pages', group: :jekyll_plugins

# Jekyll plugins
group :jekyll_plugins do
  gem 'jekyll-feed'
  gem 'jekyll-liquify'
  gem 'jekyll-paginate'
  gem 'jekyll-seo-tag'
  gem 'jekyll-sitemap'
end

gem 'rake'
